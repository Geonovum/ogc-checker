    "@stoplight/spectral-rulesets": "^1.22.0",
    "ramda": "^0.32.0",
    "@uiw/react-codemirror": "^4.25.4",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "clsx": "^2.1.1",
    "json-schema-merge-allof": "^0.8.1",
    "lucide-react": "^0.562.0",
